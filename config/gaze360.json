{
  "meta": {},
  "dataset": {
    "type": "Gaze360",
    "args": {
      "root": "/workspace/datasets/Gaze360",
      "mode": "image"
    }
  },
  "loader": {
    "type": "DataLoader",
    "args": {
      "batch_size": 16,
      "num_workers": 0
    }
  },
  "model": {
    "type": "EfficientDet",
    "args": {}
  },
  "criterion": {
    "type": "MSELoss",
    "args": {}
  },
  "evaluator": {
    "type": "AngleErrorGaze360",
    "args": {}
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 0.1
    }
  },
  "scheduler": {
    "type": "ReduceLROnPlateau",
    "args": {
      "factor": 0.1,
      "cooldown": 3
    }
  },
  "writer": {
    "type": "SummaryWriter",
    "args": {
      "log_dir": "/workspace/logs"
    }
  },
  "settings": {
    "type": "Gaze360-EfficientDet",
    "args": {
      "save": "model.pth",
      "device": "cuda:0",
      "epochs": 1000
    }
  }
}
